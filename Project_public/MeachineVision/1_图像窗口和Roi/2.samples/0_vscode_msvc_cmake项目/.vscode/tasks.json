{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/Fo${workspaceFolder}/output/build/",   //中间文件生成目录
        "/Fe${workspaceFolder}/output/exe/Debug/Project.exe", //exe生成目录
        "${file}",
        "./src/MyLibrary.cpp",
        "./src/main.cpp",
        "-I",
        "include",
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}